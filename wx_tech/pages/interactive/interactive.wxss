.clearfloat:after{
	display:block;
	clear:both;
	content:"";
	visibility:hidden;
	} 

.clearfloat{
	zoom:1
}
.not-content{
  line-height: 30rpx;
  font-size: 22rpx;
  color: #333;
}
.interacitve-box,.interactive{
	width: 100%;
	height: 100%;
	
}
.loading{
	width: 100%;
	height: 60rpx;
	background: #fafafa;
}

.loading-text{
	float:left;
	font-size: 30rpx;
}
.interactive-list{
	width: 100%;
  padding-top: 40rpx;
  padding-bottom: 30rpx;
  border-bottom: 1px solid #E1E1DF;
  font-size: 28rpx;
  background: #fff;
}
.list-left{
	float: left;
	width: 128rpx;
}
.head-icon{
	width: 80rpx;
	height: 80rpx;
	margin: 0 16rpx 0 32rpx;
	border-radius: 50%;
  vertical-align: middle;
  text-align: center;
	overflow: hidden;
}

.list-right{
	float: left;
	width: 622rpx;
	height: 100%;
	padding-right: 32rpx;
	box-sizing: border-box;
}

.class-img{
	width: 100%;
	height:  100%;
}

.title-item{
	width: 100%;
	min-height: 40rpx;
	margin-bottom: 15rpx;
	line-height: 40rpx;
	color:#666;
}

.username{
	float: left;
	margin-right: 19rpx;
	color:#333;	
}

.reply-praise{
	float: left;
}

.reply-time{
	float: right;
	height: 33rpx;
	margin-top: 7rpx;
	padding-top: 4rpx;
	font-size: 24rpx;
	
}

.reply-content{
	color: #333;
}

.publish-content{
	position: relative;
	width: 100%;
	min-height: 100rpx;
	padding: 10rpx;
	box-sizing:border-box;
	background: #F3F3F5;

}
.picture-title{
	float: left;
	width: 108rpx;
	height: 80rpx;
	margin-right: 10rpx;
	vertical-align: middle;
  text-align: center;
	overflow: hidden;
}

.text-content{
  position: absolute;
  top: 50%;
  right: 0;
  transform: translateY(-50%);
  width: 448rpx;
  padding-top: 6rpx;
	line-height: 34rpx;
  box-sizing: border-box;
	color: #333;
	overflow : hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

